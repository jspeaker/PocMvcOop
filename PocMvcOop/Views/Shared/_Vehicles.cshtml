<table id="inventory" class="table table-striped table-hover">
  <thead>
    <tr><th>Featured Vehicles</th></tr>
  </thead>
  <tbody>
    <tr><td>Loading...</td></tr>
  </tbody>
</table>

<script type="text/javascript">
  window.onload = function () {
    CarLot.init($, function () {
      CarLot.domAccess.get("/api/carlot/vehicle", function (result) {
        new CarLot.Inventory(result).render("#inventory");
      });
    });
  };
</script>